    "@babel/preset-typescript": "^7.16.7",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby-package": "0.5.3",
    "cross-env": "7.0.2",
    "eslint": "7.9.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
